{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://example.org/resident.schema.json",
  "title": "Resident",
  "type": "object",
  "required": ["id", "name", "eligibleShiftTypes"],
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1
    },
    "name": {
      "type": "string",
      "minLength": 1
    },
    "eligibleShiftTypes": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "type": "string",
        "enum": ["MOSES", "WEILER", "IP CONSULT", "NIGHT FLOAT", "BACKUP"]
      }
    }
  },
  "unevaluatedProperties": false,
  "examples": [
    {
      "id": "R17",
      "name": "P. Picard",
      "eligibleShiftTypes": ["MOSES", "WEILER", "IP CONSULT"]
    }
  ]
}
