{
  "name": "call-swap-finder",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "node ./tools/ensureRollupFallback.cjs",
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "lint": "eslint . --max-warnings=0",
    "fmt": "prettier -w .",
    "fmt:check": "prettier --check \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\" \"tools/**/*.ts\" vite.config.ts",
    "typecheck": "tsc --noEmit",
    "check:dist": "node --loader ts-node/esm tools/ensure_dist_generated.ts",
    "check:pr": "node --loader ts-node/esm tools/require_tests_changed.ts"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@tanstack/react-table": "^8.20.5",
    "dayjs": "^1.11.13",
    "fullcalendar": "^6.1.15",
    "papaparse": "^5.4.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.12.12",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@rollup/wasm-node": "^4.52.4",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^6.1.1",
    "fast-check": "^4.3.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.9",
    "vitest": "^3.2.4",
    "yaml": "^2.5.0"
  }
}
